{
    "name": "Our awesome weather extension",
    "description": "A weather application",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "./popup.html",
        "default_icon": "./icon_128.png"
    },
    "icons": {  
        "128": "./icon_128.png",
        "48": "./icon_48.png",
        "16": "./icon_16.png"
    },
   
    "permissions": ["activeTab", "geolocation"]
}


